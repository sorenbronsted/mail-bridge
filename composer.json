{
    "autoload": {
        "psr-4": {
            "bronsted\\": [
                "src/cli",
                "src/controller",
                "src/helper",
                "src/model"
            ]
        },
        "files": [
            "config/bootstrap.php",
            "config/routes.php",
            "config/appservice.php",
            "config/logging.php",
            "config/database.php",
            "config/user.php",
            "config/client.php",
            "config/mail.php",
            "config/file.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "bronsted\\": [
                "test/"
            ]
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "require": {
        "zbateson/mail-mime-parser": "^1.3",
        "slim/slim": "^4.0",
        "slim/psr7": "^1.4",
        "php-di/php-di": "^6.3",
        "monolog/monolog": "^2.2",
        "bronsted/orm": "dev-master",
        "juststeveking/http-slim": "^1.2",
        "nyholm/psr7": "^1.4",
        "symfony/http-client": "^5.3",
        "phpmailer/phpmailer": "^6.5",
        "ezyang/htmlpurifier": "^4.13"
    },
    "repositories": [{
        "type": "path",
        "url": "../orm"
    }],
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
