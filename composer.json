{
    "autoload": {
        "psr-4": {
            "bronsted\\": [
                "src/",
                "src/model"
            ]
        },
        "files": [
            "config/bootstrap.php",
            "config/routes.php",
            "config/appserver.php",
            "config/logging.php",
            "config/database.php",
            "config/user.php",
            "config/client.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "bronsted\\": [
                "test/"
            ]
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "require": {
        "ddeboer/imap": "^1.12",
        "zbateson/mail-mime-parser": "^1.3",
        "guzzlehttp/guzzle": "^7.3",
        "slim/slim": "^4.0",
        "slim/psr7": "^1.4",
        "php-di/php-di": "^6.3",
        "monolog/monolog": "^2.2",
        "bronsted/orm": "dev-master"
    },
    "repositories": [{
        "type": "path",
        "url": "../orm"
    }]
}
