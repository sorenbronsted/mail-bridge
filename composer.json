{
    "autoload": {
        "psr-4": {
            "bronsted\\": [
                "src/cli",
                "src/cli/tasks",
                "src/controller",
                "src/helper",
                "src/model"
            ]
        },
        "files": [
            "config/bootstrap.php",
            "config/routes.php",
            "config/appservice.php",
            "config/logging.php",
            "config/database.php",
            "config/user.php",
            "config/client.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "bronsted\\": [
                "test/"
            ]
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "require": {
        "zbateson/mail-mime-parser": "^1.3",
        "slim/slim": "^4.0",
        "slim/psr7": "^1.4",
        "php-di/php-di": "^6.3",
        "monolog/monolog": "^2.2",
        "bronsted/orm": "dev-master",
        "juststeveking/http-slim": "^1.2",
        "nyholm/psr7": "^1.4",
        "symfony/http-client": "^5.3",
        "phpmailer/phpmailer": "^6.5",
        "ezyang/htmlpurifier": "^4.13",
        "ruckusing/ruckusing-migrations": "^1.1",
        "hansott/psr7-cookies": "^3.0",
        "php-di/slim-bridge": "^3.1",
        "ext-imap": "*",
        "symfony/finder": "^5.3",
        "symfony/yaml": "^5.3"
    },
    "repositories": [{
        "type": "path",
        "url": "../orm"
    }],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "antecedent/patchwork": "^2.1"
    }
}
