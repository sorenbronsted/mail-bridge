{
    "autoload": {
        "psr-4": {
            "bronsted\\": [
                "src/cli",
                "src/cli/tasks",
                "src/controller",
                "src/helper",
                "src/model",
                "src/task"
            ]
        },
        "files": [
            "config/bootstrap.php",
            "config/routes.php",
            "config/config.php",
            "config/logging.php",
            "config/database.php",
            "config/user.php",
            "config/client.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "bronsted\\": [
                "test/"
            ]
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "require": {
        "slim/slim": "^4.0",
        "slim/psr7": "^1.4",
        "php-di/php-di": "^6.3",
        "monolog/monolog": "^2.2",
        "bronsted/orm": "^2.0",
        "juststeveking/http-slim": "^1.2",
        "nyholm/psr7": "^1.4",
        "symfony/http-client": "^5.3",
        "ezyang/htmlpurifier": "^4.13",
        "ruckusing/ruckusing-migrations": "^1.1",
        "hansott/psr7-cookies": "^3.0",
        "php-di/slim-bridge": "^3.1",
        "ext-imap": "*",
        "symfony/finder": "^5.3",
        "symfony/yaml": "^5.3",
        "symfony/mailer": "^5.4",
        "zbateson/mail-mime-parser": "^2.1",
        "react/http": "^1.5",
        "react/promise-timer": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "antecedent/patchwork": "^2.1"
    },
    "repositories": [
        {"type": "vcs","url": "https://github.com/sorenbronsted/orm"}
    ]
}
